#: de/cubeisland/engine/spawn/SpawnCommands.java:60
msgid "Changes the respawnpoint"
msgstr "Verschiebt den Respawn-Punkt"

#: de/cubeisland/engine/spawn/SpawnCommands.java:151
msgid "Teleport directly to the worlds spawn."
msgstr "Teleport direkt zum Welt-Spawn."

#: de/cubeisland/engine/spawn/SpawnCommands.java:60
msgid "[<role>|global] [<x> <y> <z>] [world]"
msgstr "[<Rolle>|global] [<x> <y> <z>] [world]"

#: de/cubeisland/engine/spawn/SpawnCommands.java:151
msgid "[player] [world <world>] [role <role>]"
msgstr "[player] [world <Welt>] [role <Rolle>]"

#: de/cubeisland/engine/spawn/SpawnCommands.java:100
msgid "Coordinates are invalid!"
msgstr "Die Koordinaten sind ungültig!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:126
#: de/cubeisland/engine/spawn/SpawnCommands.java:263
msgid "Could not find the role {input} in {world}!"
msgstr "Rolle {input} wurde in {world} nicht gefunden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:87
#: de/cubeisland/engine/spawn/SpawnCommands.java:108
msgid "If not used ingame you have to specify a world and coordinates!"
msgstr "Wenn nicht im Spiel benutzt, müssen eine Welt und die Koordinaten angeben werden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:210
msgid ""
"Invalid spawn-location for the role of {user}! Please check your role-configurations!\n"
"{}"
msgstr "Ungültige Spawn-Position für die Rolle von {user}! Bitte überprüfe deine Rollenkonfigurationen!\n{}"

#: de/cubeisland/engine/spawn/SpawnCommands.java:275
msgid ""
"Invalid spawn-location for the role {name}! Please check your role-configuration!\n"
"{}"
msgstr "Ungültige Spawn-Position für die Rolle {name}! Bitte überprüfe deine Rollenkonfiguration!\n{}"

#: de/cubeisland/engine/spawn/SpawnCommands.java:301
msgid ""
"Invalid spawn-location for your role! Please check your role-configurations!\n"
"{}"
msgstr "Ungültige Spawn-Position für die Rolle! Bitte überprüfe deine Rollenkonfiguration!\n{}"

#: de/cubeisland/engine/spawn/SpawnCommands.java:269
msgid "The role {name} in {world} has no spawn-point!"
msgstr "Die Rolle {name} in {world} hat keinen Spawn-Punkt!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:148
msgid "The spawn in {world} is now set to {vector}"
msgstr "Der Spawn in {world} liegt nun bei {vector}"

#: de/cubeisland/engine/spawn/SpawnCommands.java:88
#: de/cubeisland/engine/spawn/SpawnCommands.java:109
msgid "Use {text:global} instead of the role-name to set the default spawn."
msgstr "Benutze {text:global} anstatt der Rollen-Namen um den Standard-Spawn zu setzen."

#: de/cubeisland/engine/spawn/SpawnCommands.java:242
msgid "User {user} not found!"
msgstr "Benutzer {user} nicht gefunden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:79
msgid "World {input} not found"
msgstr "Welt {input} nicht gefunden"

#: de/cubeisland/engine/spawn/SpawnCommands.java:178
msgid "World {input} not found!"
msgstr "Welt {input} nicht gefunden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:191
msgid "You are not allowed to spawn everyone!"
msgstr "Du darfst nicht alle spawnen!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:252
msgid "You are not allowed to spawn {user}!"
msgstr "Du darfst {user} nicht spawnen!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:278
msgid "You are now standing at the spawn of {name#role}!"
msgstr "Du stehst nun am Spawn der {name#role}!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:247
msgid "You cannot teleport an offline player to spawn!"
msgstr "Du kannst keinen Offline-Spieler an den Spawn teleportieren!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:184
msgid "You have to specify a world!"
msgstr "Du musst eine Welt angeben!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:234
msgid "{text:ProTip}: Teleport does not work IRL!"
msgstr "{text:ProTip}: Teleportation klappt nicht IRL!"
